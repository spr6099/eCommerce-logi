
    <div class="col-10">
        <div class="container" style="height: 250px;width:400px">
            <h2>add Subcategory</h2>

            <form action="" method="post" class="container border-2 border-dark ">
                Select category
                <select name="category"  class="form-control border-3 border-dark">
                    <option>select</option>
                    {{#each category}}
                    <option value="{{this._id}}">{{this.catName}}</option>
                    {{/each}}
                </select>
                Enter Subcategory
                <input type="text" name="subCategory" id="" class="form-control border-3 border-dark">
                <button class="btn btn-dark p-2 m-2">submit</button>
            </form>
        </div>
       
    </div>




